package Abstraction;

public class ChildAirbb extends ParentAircraft{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	ChildAirbb bb = new ChildAirbb();
	ParentAircraft zz = new ChildAirbb(); //dynamic polymorphism
	bb.engine();
	bb.safetyguildlines();
	bb.bodycolor();
	zz.safetyguildlines();
	zz.engine();
	
	
	}
	
//	@Override
//	public void bodycolor() {
//		// TODO Auto-generated method stub
//		System.out.println("body of the color is Black");
	}

	@Override
	public void safetyguildlines()
	{
		System.out.println("Safety GuildLines child");
	}
	@Override
	public void engine()
	{
		System.out.println("ABC child");
		
	}

	
}
