package JavaInterviewQuesAndAns;

public class DefaultConstractor {

	String student_name;
	int roll_num;
//	/-------------------Default constructor------------------/
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		DefaultConstractor constractor = new DefaultConstractor();
		System.out.println(constractor.student_name);
		System.out.println(constractor.roll_num);
	}

}
