
public class Corejava4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		//while loop
		
		int i=10;
		while (i>0)
		{
			System.out.println(i);
			i--;
		}
		
		
		//do while loop
		
		int j=50;
		do
		{
			j++;
		}while(j<60);
		System.out.println(j);
			
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
